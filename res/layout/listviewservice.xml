<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout01"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="4dip"
    android:paddingLeft="12dip"
    android:paddingRight="12dip" >

    <TextView
        android:id="@+id/serviceNumcount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No Date" />

    <TextView
        android:id="@+id/serviceName"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/serviceNumcount"
        android:text="No Date" />

    <TextView
        android:id="@+id/serviceCost"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/serviceName"
        android:layout_toRightOf="@+id/serviceNumcount"
        android:text="No Date" />

    <TextView
        android:id="@+id/serviceRemarks"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/serviceCost"
        android:layout_toRightOf="@+id/serviceNumcount"
        android:text="No Date" />
    
    <TextView
        android:id="@+id/serviceChangecode"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/serviceRemarks"
        android:layout_toRightOf="@+id/serviceNumcount"
        android:text="No Date" />

</RelativeLayout>