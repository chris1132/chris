<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout01"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="4dip"
    android:paddingLeft="12dip"
    android:paddingRight="12dip" >

    <TextView
        android:id="@+id/oncenumcount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No Date" />

    <TextView
        android:id="@+id/onceItemcontent"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/oncenumcount"
        android:text="No Date" />

    <TextView
        android:id="@+id/onceItemTime"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/onceItemcontent"
        android:layout_toRightOf="@+id/oncenumcount"
        android:text="No Date" />
    
    <TextView
        android:id="@+id/onceItemState"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/onceItemTime"
        android:layout_toRightOf="@+id/oncenumcount"
        android:text="No Date" />

</RelativeLayout>
